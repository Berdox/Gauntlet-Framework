{
	"Name" : "Berdox.gauntlet_framework",
	"Description": "A framework built for titanfall 2 to manage the start, finish, checkpoints, timer, and other UI elements for map",
	"Version": "0.5.5",
	"LoadPriority": 4,
	"RequiredOnClient": true,
	
	"Scripts": [
		{
            "Path": "sv_gauntlet.nut",
            "RunOn": "SERVER && MP",
			"ServerCallback": {
				"After": "Gauntlet_ServerStart",
				"Before": "Gauntlet_NetworkRegistration"
			}
        },
		{
            "Path": "cl_gauntlet.nut",
            "RunOn": "CLIENT && MP",
			"ClientCallback": {
				"Before": "Gauntlet_NetworkRegistration"
			}
        }
	]
}